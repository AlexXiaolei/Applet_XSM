<view>
  <button bindtap="bindAddStore">添加门店</button>
  <view wx:for="{{store_list}}">
    <view class="store_info" data-id="{{item.ID}}" bindtap="bindEditStore" >
      <text>店名：{{item.StoreName}}</text>
      <text>地址：{{item.StoreAddress}}</text>
      <text>电话：{{item.StoreTel}}</text>
      <text>{{item.Remark}}</text>
      <image src="images/map.jpeg"></image>
    </view>
  </view>
</view>